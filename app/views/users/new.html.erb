<% provide(:title, 'Sign Up') %>
<h1>Sign Up</h1>
<div class="row">
  <div class="col-sm-4 col-sm-offset-4">
    <%= form_for(@user) do |f| %>
      <%= render 'shared/error_messages', object: f.object %>
      <%= render 'users/field', f:f %>
      <%= f.submit 'Create My Account', class: 'btn btn-default btn-primary' %>
    <% end %>
  </div>
</div>